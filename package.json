		"svelte-check": "^3.8.6",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0",
		"vite": "^5.4.19",
		"vitest": "^2.1.9",
		"wrangler": "^3.114.11"
	},
	"type": "module",
	"dependencies": {
		"bigger-picture": "^1.1.19",
		"feed": "^4.2.2",
		"macy": "^2.5.1",
		"remark-footnotes": "~2.0.0",
		"svelte-icons-pack": "^3.1.3",
		"sveltekit-i18n": "^2.4.2",
		"uuid": "^10.0.0"
